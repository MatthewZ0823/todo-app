    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.2.1",
    "@sveltejs/vite-plugin-svelte": "^2.0.4",
    "@tsconfig/svelte": "^5.0.0",
    "@types/electron": "^1.6.10",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "daisyui": "^3.1.9",
    "electron": "^25.2.0",
    "postcss": "^8.4.24",
    "svelte": "^3.58.0",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
